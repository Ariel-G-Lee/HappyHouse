<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
	<mapper namespace="com.mycom.house.dao.LoginDao">
	
	<select id="login" parameterType="string" resultType="com.mycom.house.dto.UserDto">
		SELECT USER_ID, USER_NAME, USER_PWD, EMAIL, ADDRESS, JOINDATE
		FROM USER
		WHERE USER_ID = #{userId}
	</select>

</mapper>