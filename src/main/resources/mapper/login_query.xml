<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd"> 
	<mapper namespace="com.ssafy.house.dao.LoginDao">
	
	<select id="login" parameterType="string" resultType="com.ssafy.house.dto.UserDto">
		SELECT *
		FROM USER
		WHERE USER_ID = #{userId}
	</select>

</mapper>